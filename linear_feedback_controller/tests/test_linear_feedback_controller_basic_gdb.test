<launch>
    <test test-name="LinearFeedbackControllerBasic"
          pkg="linear_feedback_controller"
          type="test_linear_feedback_controller_basic"
          launch-prefix="xterm -sl 9999999 -e gdb --args"
          time-limit="1000000000.0">
      <env name="ROSCONSOLE_CONFIG_FILE"
         value="$(find linear_feedback_controller)/tests/rosconsole.conf"/>
      <env name="CTEST_OUTPUT_ON_FAILURE" value="1" />
    </test>
</launch>